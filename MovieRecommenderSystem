   ],
   "source": [
    "import pandas as pd\n",
    "import numpy as np\n",
    "from sklearn.feature_extraction.text import TfidfVectorizer\n",
    "from sklearn.metrics.pairwise import cosine_similarity\n",
    "import matplotlib.pyplot as plt\n",
    "import seaborn as sns\n",
    "\n",
    "# Load the movies dataset\n",
    "movies_df = pd.read_csv('/kaggle/input/alx-movie-recommendation-project-2024/movies.csv')\n",
    "\n",
    "# Take a smaller sample of the movies dataset to avoid memory issues\n",
    "sample_size = 1000\n",
    "sample_movies_df = movies_df.sample(n=sample_size, random_state=42).reset_index(drop=True)\n",
    "\n",
    "# Extract year from title and create a combined feature\n",
    "sample_movies_df['year'] = sample_movies_df['title'].str.extract('(\\d{4})').astype(float)\n",
    "sample_movies_df['features'] = sample_movies_df['genres'].fillna('') + ' ' + sample_movies_df['year'].astype(str)\n",
    "\n",
    "# Create and fit the TF-IDF vectorizer\n",
    "tfidf = TfidfVectorizer(stop_words='english')\n",
    "tfidf_matrix = tfidf.fit_transform(sample_movies_df['features'])\n",
    "\n",
    "# Compute the cosine similarity matrix\n",
    "cosine_sim = cosine_similarity(tfidf_matrix, tfidf_matrix)\n",
    "\n",
    "def get_recommendations(title, cosine_sim, df):\n",
    "    # Get the index of the movie that matches the title\n",
    "    idx = df.index[df['title'] == title].tolist()\n",
    "    if not idx:\n",
    "        return pd.DataFrame()  # Return an empty DataFrame if movie not found\n",
    "    idx = idx[0]\n",
    "    \n",
    "    # Get the pairwise similarity scores of all movies with that movie\n",
    "    sim_scores = list(enumerate(cosine_sim[idx]))\n",
    "    \n",
    "    # Sort the movies based on the similarity scores\n",
    "    sim_scores = sorted(sim_scores, key=lambda x: x[1], reverse=True)\n",
    "    \n",
    "    # Get the scores of the 10 most similar movies\n",
    "    sim_scores = sim_scores[1:11]\n",
    "    \n",
    "    # Get the movie indices\n",
    "    movie_indices = [i[0] for i in sim_scores]\n",
    "    \n",
    "    # Return the top 10 most similar movies\n",
    "    return df.iloc[movie_indices]\n",
    "\n",
    "# Get a random movie from our sample\n",
    "random_movie = sample_movies_df['title'].sample().iloc[0]\n",
    "print(f\"Random movie selected: {random_movie}\")\n",
    "\n",
    "# Get recommendations for the random movie\n",
    "recommendations = get_recommendations(random_movie, cosine_sim, sample_movies_df)\n",
    "print(f\"\\nRecommendations for {random_movie}:\")\n",
    "print(recommendations[['title', 'genres', 'year']])\n",
    "\n",
    "# Analyze the genre distribution\n",
    "genre_counts = sample_movies_df['genres'].str.split('|').explode().value_counts()\n",
    "plt.figure(figsize=(12, 6))\n",
    "sns.barplot(x=genre_counts.index, y=genre_counts.values, palette='viridis')\n",
    "plt.title('Genre Distribution in Sample Movies')\n",
    "plt.xlabel('Genre')\n",
    "plt.ylabel('Count')\n",
    "plt.xticks(rotation=45, ha='right')\n",
    "plt.tight_layout()\n",
    "plt.show()\n",
    "\n",
    "# Analyze the distribution of movie release years\n",
    "plt.figure(figsize=(12, 6))\n",
    "sns.histplot(sample_movies_df['year'].dropna(), bins=30, kde=True)\n",
    "plt.title('Distribution of Movie Release Years')\n",
    "plt.xlabel('Year')\n",
    "plt.ylabel('Count')\n",
    "plt.tight_layout()\n",
    "plt.show()\n",
    "\n",
    "# Print dataset info and sample\n",
    "print(sample_movies_df.info())\n",
    "print(\"\\nSample of the data:\")\n",
    "print(sample_movies_df.head())\n",
    "\n",
    "# Get recommendations for a specific movie\n",
    "specific_movie = sample_movies_df['title'].iloc[0]  # Use the first movie in the dataset\n",
    "print(f\"\\nGetting recommendations for: {specific_movie}\")\n",
    "improved_recommendations = get_recommendations(specific_movie, cosine_sim, sample_movies_df)\n",
    "print(f\"\\nRecommendations for {specific_movie}:\")\n",
    "print(improved_recommendations[['title', 'genres', 'year']])"
   ]
  }
